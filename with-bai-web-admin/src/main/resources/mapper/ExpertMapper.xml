<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.with.bai.admin.dao.ExpertDao">


    <insert id="save">
        INSERT INTO
        wb_expert(
        name,
        image,
        detail
        )values(
        #{name},
        #{image},
        #{detail}
        )
    </insert>
</mapper>