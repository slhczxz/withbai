        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--
      namespace；隔离sql语句
-->
<mapper namespace="com.with.bai.dao.ExpertPageMapper">

    <select id="getExpert" resultType="Expert">
        select
            eid,
            name,
            image,
            detail
        from wb_expert


    </select>

